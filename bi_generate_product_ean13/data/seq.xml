<odoo>
	<data>
        <record id="product_number" model="ir.sequence">
            <field name="name">product number</field>
            <field name="code">product.number</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="product_code" model="ir.sequence">
            <field name="name">product code</field>
            <field name="code">product.code</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>